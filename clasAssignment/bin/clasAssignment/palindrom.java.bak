package clasAssignment;

class palindrom{
	public static void main(String[] args) {
		int n =12,rem,a=0,temp=n;
		while (n>0) {
			rem=n%10;
			a=a*10+rem;
			n=n/10;
		}
		if (temp==a) {
			System.out.println("palindrom");
		}
	}
}

